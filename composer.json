{
    "name": "isdgroup/isdcore",
    "description": "ISD Group Core functional",
    "keywords": [
        "league",
        ":package_name"
    ],
    "homepage": "https://github.com/Assada/isd-core",
    "license": "MIT",
    "authors": [
        {
            "name": "Assada",
            "email": "assada.ua@gmail.com",
            "homepage": "https://assada.me",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Isdgroup\\Isdcore\\": "src"
        }
    },
    "autoload-dev": {
